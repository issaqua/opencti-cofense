opencti:
  url: 'http://localhost:8080'
  token: 'ChangeMe'

connector:
  id: "ChangeMe"
  type: "EXTERNAL_IMPORT"
  name: "Cofense"
  scope: "Cofense"
  log_level: "info"
  confidence_level: 100
  update_existing_data: true
  interval: 86400
  loop_interval: 600

ironnet:
  api_url: "https://www.threathq.com/apiv1"
  api_key: "ChangeMe"
  verify: true
  create_indicators: true
  create_ip_indicators: true
  ip_indicator_valid_until: "P90D"  # 90 days
